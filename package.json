{
  "name": "go-to-test",
  "displayName": "go-to-test",
  "description": "Go to tests of current file",
  "version": "0.1.0",
  "publisher": "yrachid",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:go-to-test.openTestFile",
    "onCommand:go-to-test.openTestFileBeside"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "go-to-test.openTestFile",
        "title": "Open test file"
      },
      {
        "command": "go-to-test.openTestFileBeside",
        "title": "Open test file beside"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "ts-loader": "^9.2.5",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "@vscode/test-electron": "^1.6.2"
  }
}
